{% comment %}
  <!-- ========================================================= DYNAMIC TITLE CODE ========================================================= -->
   
          {% if product.metafields.custom.option_1_alt_titles %} <!-- If the option1 metafield has a value, run the following code that checks for validity and structure -->
            {% assign product_option_one_values = '' %}
            
            {% for product_option in product.options_with_values %} <!-- Grabbing the option values for the first option and placing them into an array for the next conditional check --> 
              {% if forloop.index == 1 %}
                {% for value in product_option.values %}
                  {% if forloop.last %}
                    {% assign product_option_one_values = product_option_one_values | append: value %}
                  {% else %}
                    {% assign product_option_one_values = product_option_one_values | append: value | append: ", "%}
                  {% endif %}                 
                {% endfor %}
                  {% endif %}
            {% endfor %}
            
            {% assign product_option_one_values_array = product_option_one_values | split: ", " %}
            {% assign titles_array = product.metafields.custom.option_1_alt_titles | split: '//' %}

            {% if titles_array.size == product_option_one_values_array.size %} <!-- Continue if the size of the option1 value array is the same as the titles_array (meaning the titles_array was properly formatted) -->
              {% assign options_w_titles = '' %}
              {% for option_1 in product_option_one_values_array %}
                {% if forloop.last %}
                  {% assign options_w_titles = options_w_titles | append: product_option_one_values_array[forloop.index0] | append: " --- " | append: titles_array[forloop.index0]%}
                {% else %}
                  {% assign options_w_titles = options_w_titles | append: product_option_one_values_array[forloop.index0] | append: " --- " | append: titles_array[forloop.index0] | append: ", "%}
                {% endif %}
              {% endfor %}

             

              {% assign title_to_use = product.title %}
              {% assign options_w_titles_array = options_w_titles | split: "," %}
              {% for option_title_combo in options_w_titles_array %}
                
                {% if option_title_combo contains product.selected_or_first_available_variant.option1 %}
                  {% assign option_title_combo_array = option_title_combo | split: ' --- ' %}
                  {% assign title_to_use = option_title_combo_array[1] %}
                  
                  {% break %}
                {% endif %}
              {% endfor %}

              {% assign dynamic_title = title_to_use %}

              {% if title_to_use == 'default' %}
                {% assign dynamic_title = product.title %}
              {% endif %}
              
              

             

              <script id="alt-titles-container" type="application/json">{{ options_w_titles }}</script> <!-- Renders a script holding the option value names with alt titles for comparison in the updateTitle function in global.js --> 

             
            {% endif %}

            <script>console.log('{{ dynamic_title }}')</script>

           

            {% comment %}
              <script>console.log({{ options_w_titles | split: ", " | json}})</script>            
              <script>console.log({{ product_option_one_values | split: ", " | json}})</script>
            {% endcomment %}

            {% else %} <!-- If the product does not have a value in the alt title metafield, just render the normal title and do no produce the array for global.js to do it's work (meaning nothing will happen when variants change.)

            {% assign dynamic_title = product.title %}
          {% endif %}

