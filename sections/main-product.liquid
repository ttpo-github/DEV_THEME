      {% if dynamic_green_light %}
              {% for metaobject in product.metafields.custom.alt_titles_and_descriptions.value %}
                {% assign metaObjTitle = metaobject.alternate_title | metafield_text %}
                {% if metaObjTitle == 'DEFAULT' or metaObjTitle == blank %}
                  <span {% if product_option_one_values_array[forloop.index0] contains product.selected_or_first_available_variant.option1 %}{% else %}style="display:none;"{% endif %} data-product-option-one="{{ product_option_one_values_array[forloop.index0] }}" class="ttpo_product_title">{{product.title}}</span>
                {% else %}
                  <span {% if product_option_one_values_array[forloop.index0] contains product.selected_or_first_available_variant.option1 %}{% else %}style="display:none;"{% endif %} data-product-option-one="{{ product_option_one_values_array[forloop.index0] }}" class="ttpo_product_title">{{metaobject.alternate_title | metafield_text}}</span>
                {% endif %}
              {% endfor %}
  
              {% else %}
  
              {{ product.title }}
              
             {% endif %}
    </h1>
  </div>
  {% assign variant_images = product.images | where: 'attached_to_variant?', true | map: 'src' %}

  <div class="product product-single product--{{ section.settings.media_size }} product--{{ section.settings.media_position }} product--{{ section.settings.gallery_layout }} product--mobile-{{ section.settings.mobile_thumbnails }} grid grid--1-col {% if product.media.size > 0 %}grid--2-col-tablet{% else %}product--no-media{% endif %}">
    <div class="grid__item product__media-wrapper{% if section.settings.media_position == 'right' %} medium-hide large-up-hide{% endif %}">
      {% render 'product-media-gallery', variant_images: variant_images %}

      {% comment %}
      <div class="helps" id="myBtn">
        <div class="help-images">
          <img src="{{ section.settings.help-image | img_url: 'master' }}">
        </div>
        
        <div class="help-content">
          <h2>{{ section.settings.help-heading }}</h2>
          <div class="click">
            <div data-name="Talk To [name] or another Trailer Parts Expert now!">Talk To Krystal or another Trailer Parts Expert now! </div>
          </div>
        </div>      
      </div>
      {% endcomment %}

     
      
    </div>
    <div class="product__info-wrapper grid__item{% if settings.page_width > 1400 and section.settings.media_size == "small" %} product__info-wrapper--extra-padding{% endif %}">
      <product-info
        id="ProductInfo-{{ section.id }}"
        data-section="{{ section.id }}"
        data-url="{{ product.url }}"
        class="product__info-container{% if section.settings.enable_sticky_info %} product__column-sticky{% endif %}"
      >
        {%- assign product_form_id = 'product-form-' | append: section.id -%}

        

        {% if product.title contains 'Kit' %}
          {% assign product_is_kit = true %}
        {% else %}
          {% assign product_is_kit = false %}
        {% endif %}


        
        
        

        
        
        {% comment %}
         {% if product.selected_or_first_available_variant.option1 contains 'Kit' and product_is_kit == false %}
          <!-- Kit Title Finder -->
            {% assign collection_handle = 'tk-trailer-kits' %}
            {% assign sku_to_find = product.selected_or_first_available_variant.sku %}
            {% assign found_product = null %}
            {% assign collection = collections[collection_handle] %}
        
                
                {% for product_item in collection.products %}
                  {% if product_item.id != product.id %}
                  {% for variant in product_item.variants %}
                    {% if variant.sku == sku_to_find %}
                      {% assign found_product = product_item %}
                      {% break %}
                    {% endif %}
                  {% endfor %}
                  {% if found_product %}
                    {% break %}
                  {% endif %}
                  {% endif %}
                {% endfor %}
                
                {% if found_product %}
                  <script>console.log("PRODUCT FOUND")</script>
                  <script>console.log({{found_product | json}})</script>
                  {% assign dynamic_title = found_product.title %}
                {% endif %}
              <!-- Kit Title Finder --> 
          {% endif %}
        {% endcomment %}

       

        {% comment %}
          {% assign titles_array = product.metafields.custom.option_1_alt_titles | split: '//' %}
          {% assign product_options = product.options %}
          {% assign options_length = product.options.length %}

          <script>console.log({{ titles_array | json}})</script>
          <script>console.log({{ product_options | json}})</script>
          <script>console.log('{{ options_length }}')</script>
